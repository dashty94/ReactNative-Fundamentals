{
    "expo": {
        "name": "DoneWithIt",
        "slug": "DoneWithIt",
        "privacy": "unlisted",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/icon.png",
        "splash": {
            "image": "./app/assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#e63c4b"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "description": "A market place for selling the things you do not want anymore",
        "notification": {
            "iosDisplayInForeground": true
        }
    }
}
